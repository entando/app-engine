<?xml version="1.0" encoding="UTF-8"?>
<component>
	<code>jpwebform</code>
	<description>Form</description>
	<installation>
		<tableMapping>
			<datasource name="servDataSource">
				<class>it.difesa.esercito.plugins.jpwebform.aps.system.init.servdb.Form</class>
			</datasource>
		</tableMapping>
		<environment code="production">
			<defaultSqlResources>
				<datasource name="portDataSource">
					classpath:sql/plugins/jpwebform/port_data_production.sql
				</datasource>
				<datasource name="servDataSource">
					classpath:sql/plugins/jpwebform/serv_data_production.sql
				</datasource>
			</defaultSqlResources>
		</environment>
		<environment code="test">
			<defaultSqlResources>
				<datasource name="portDataSource">
					classpath:sql/plugins/jpwebform/port_data_test.sql
				</datasource>
				<datasource name="servDataSource">
					classpath:sql/plugins/jpwebform/serv_data_test.sql
				</datasource>
			</defaultSqlResources>
		</environment>
	</installation>
</component>
